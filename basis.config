{
  "server": {
    "port": 8000
  },
  "plugins": [
    {
      "name": "basisjs-tools-instrumenter",
      "target": "server",
      "ignore": [
        "build/**",
        "node_modules/**"
      ],
      "options": {
        "blackbox": [
          "/node_modules/**",
          "/lib/*",
          "/src/*",
          "/src/**"
        ]
      }
    }
  ]
}
